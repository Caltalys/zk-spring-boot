<?xml version="1.0" encoding="UTF-8"?>
<zk>
	<config-name>zk-spring-boot</config-name>
	<!--<client-config>-->
	<!--<debug-js>true</debug-js>-->
	<!--</client-config>-->

	<!--<library-property>-->
	<!--<name>org.zkoss.bind.DebuggerFactory.enable</name>-->
	<!--<value>true</value>-->
	<!--</library-property>-->


	<richlet>
		<richlet-name>TestRichlet</richlet-name><!-- your preferred name -->
		<richlet-class>zk.springboot.TestRichlet</richlet-class><!-- your class name, of course -->
	</richlet>
	<richlet-mapping>
		<richlet-name>TestRichlet</richlet-name>
		<url-pattern>/test</url-pattern>
	</richlet-mapping>

	<!--enable websocket support in ZK 8.5  -->
	<listener>
		<listener-class>zk.springboot.SpringBootWebSocketWebAppInit</listener-class>
	</listener>
</zk>
